{
    "python.formatting.provider": "black",
    "files.exclude": {
        "**/env": true,
        "**/__pycache__": true,
        "**/locust_plugins.egg-info": true,
        "**/pip-wheel-metadata": true,
        "**/build": true,
        "**/dist": true,
        "**/src": true,
        "**/.eggs": true,
        "**/.metals": true,
        "**/.mypy_cache": true,
        "**/.tox": true,
        "**/project/": true,
        "har2locust/tests/.pytest_cache/**/*": true,
        ".pytest_cache/**/*": true,
        "**/.python-version": true,
        "**/_version.py": true,
        "examples/screenshot_*": true,
        "**/output.txt": true,
        "**/.pytest_cache": true
    },
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--init-hook",
        "import sys; sys.path.insert(0, '.')"
    ],
    "editor.formatOnSave": true,
    "python.linting.enabled": true
}